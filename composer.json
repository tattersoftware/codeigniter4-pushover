{
	"name": "tatter/pushover",
	"description": "Pushover integration for CodeIgniter 4",
	"keywords": [
		"codeigniter",
		"codeigniter4",
		"pushover",
		"notifications",
		"push",
		"mobile"
	],
	"homepage": "https://github.com/tattersoftware/codeigniter4-pushover",
	"license": "MIT",
	"authors": [
		{
			"name": "Matthew Gatner",
			"email": "mgatner@tattersoftware.com",
			"homepage": "https://tattersoftware.com",
			"role": "Developer"
		}
	],
	"repositories": [
		{
			"type": "vcs",
			"url": "https://github.com/codeigniter4/CodeIgniter4"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"require": {
		"php" : ">=7.2"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"fzaninotto/faker": "^1.9@dev",
		"codeigniter4/codeigniter4": "dev-develop"
	},
	"autoload": {
		"psr-4": {
			"Tatter\\Pushover\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Support\\": "tests/_support"
		}
	},
	"scripts": {
		"test": "phpunit",
		"post-update-cmd": [
			"composer dump-autoload"
		]
	}
}
